// room script file


function oQuitButton_AnyClick(Object *theObject, CursorMode mode)
{
QuitGame(1);
}

function oStartButton_AnyClick(Object *theObject, CursorMode mode)
{
cEgo.ChangeRoom(7, 505,  250);
}
function room_Load()
{
aMenuMusic.Play();
mouse.Mode = eModePointer;
mouse.DisableMode(eModeInteract);
mouse.DisableMode(eModeLookat);
mouse.DisableMode(eModeTalkto);
mouse.DisableMode(eModeWalkto);
mouse.DisableMode(eModeUsermode1);
mouse.DisableMode(eModeUsermode2);
mouse.DisableMode(eModeWait);
gIconbar.Visible = false;
}
